{
  "name": "My Lamp Server",
  "dockerComposeFile": "docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/var/www/html",
  "customizations": {
    "vscode": {
      "extensions": [
        "felixfbecker.php-debug",
        "felixfbecker.php-intellisense"
      ],
      "settings": {
        "php.validate.executablePath": "/usr/local/bin/php"
      }
    }
  }
}